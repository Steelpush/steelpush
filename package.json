{
  "name": "steelpush",
  "version": "0.1.0",
  "description": "AI-powered website optimization tool",
  "main": "dist/index.js",
  "bin": {
    "steelpush": "./dist/cli.js",
    "steelpush-source": "./dist/cli-source-analyzer.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "tsx src/test-scanner.ts",
    "dev:source": "tsx src/cli-source-analyzer.ts",
    "dev:scan": "tsx src/analyze-usetrag.ts",
    "dev:scan:enhanced": "tsx src/analyze-usetrag-enhanced.ts",
    "dev:scan:direct": "tsx src/analyze-usetrag-direct.ts",
    "dev:scan:mini": "tsx src/analyze-usetrag-mini.ts",
    "dev:scan:mcp": "tsx src/analyze-usetrag-mcp.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "ai",
    "optimization",
    "website",
    "conversion",
    "simulation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.11",
    "@ai-sdk/openai": "^1.3.21",
    "@mastra/core": "^0.9.1",
    "@mastra/mcp": "^0.4.3",
    "@playwright/mcp": "^0.0.22",
    "ai": "^4.3.15",
    "commander": "^11.0.0",
    "glob": "^11.0.2",
    "inquirer": "^12.6.1",
    "langchain": "^0.1.0",
    "mastra": "^0.6.0",
    "openai": "^4.0.0",
    "playwright": "^1.52.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.0.0"
  },
  "type": "module"
}
